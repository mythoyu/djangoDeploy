{
  "builds": [
    {
      "src": "post/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "post/wsgi.py"
    }
  ],
  "env": {
    "DATABASE_URL": "postgresql://username:password@hostname:port/dbname"
  }
}
